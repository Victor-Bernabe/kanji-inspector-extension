{
  "name": "Kanji Inspector",
  "description": "Highlight and right click to find information about Kanji.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["contextMenus"],
  "background": {
    "scripts": [ "websites.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "instructions.html"
  },
  "icons": {
   "16": "icon16x16.png",
   "48": "icon48x48.png",
  "128": "icon128x128.png"
 }
}
